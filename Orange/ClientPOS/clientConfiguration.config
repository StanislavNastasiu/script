    EpsonFP90   => "EPSON CashDrawer"
    WincorTH230 => "WN_KA.FPTR_ND77"
	IBMSurePOS700 => "IBM_CD_SurePOS700"
    CustomKubeF => "FP.CustomKubeF"
    DitronZIP   => "FiscalPrinterSO.DrawerInterface"
	Ditron Synthes=> "FP.Ditron"
